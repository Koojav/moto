#!/usr/bin/env ruby

require_relative '../lib/test_logging'
require_relative '../lib/test/base'

# require all listeners defined in moto/lib/reporting/listeners
Dir[File.dirname(File.dirname(__FILE__)) + '/lib/reporting/listeners/*.rb'].each do |file|
  require file
end

require_relative '../lib/parameter_parser'
Moto::ParameterParser.parse_user_input(ARGV)
